<div class="card">
  <h3 class="card-header">Create an account</h3>
  <form id="loginForm" class="card-body">
    <label for="email-create">User Email:</label>
    <input class="form-input" type="text" id="email-create" />

    <label for="password-create">User Password:</label>
    <input class="form-input" type="text" id="password-create" />

    <button type="submit" class="btn">Submit!</button>
    <a class="create-account" href="/login">Already have an account?
      Click here to Login.</a>
  </form>
</div>

<script src="/public/JS/createAccount.js"></script>